{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "url": [
        "https://studio-m-pearl.vercel.app/",
        "https://studio-m-pearl.vercel.app/portfolio",
        "https://studio-m-pearl.vercel.app/contato"
      ],
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.85 }],
        "categories:accessibility": ["warn", { "minScore": 0.9 }],
        "categories:seo": ["warn", { "minScore": 0.9 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
